\def\@xendnote[#1]{\begingroup \c@endnote=#1\relax
   \xdef\@theenmark{\theendnote}\endgroup
   \@endnotemark\@endnotetext}
 
%  Here begins endnote code that's really different from the footnote
% code of LaTeX.
 
\let\@doanenote=0
\let\@endanenote=0
 
